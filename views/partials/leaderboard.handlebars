<div id="stream" style="display: flex; justify-content: center;" class="col-xs-12 col-sm-12">
	<div class="col-xs-12 col-sm-6">
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h3 class="panel-title">Classement des joueurs</h3>
			</div>
			<div class="panel-body">
				<table class="table table-striped">
					<thead>
						<tr>
						<th scope="col">Pseudo</th>
						<th scope="col">Points</th>
						</tr>
					</thead>
					<tbody>
					{{#each users}}
						<tr>
						<td>{{login}}</td> <!-- Afficher le login de l'utilisateur -->
						<td>{{points}}</td> <!-- Afficher les points de l'utilisateur -->
						</tr>
					{{/each}}
					</tbody>
					</table>
			</div>
		</div>
	</div>
</div>